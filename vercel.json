{
    "version": 2,
    "routes": [
        {
            "src": "/pages/admin/(.*)",
            "dest": "/pages/admin/$1"
        },
        {
            "src": "/pages/auth/(.*)",
            "dest": "/pages/auth/$1"
        },
        {
            "src": "/pages/user/(.*)",
            "dest": "/pages/user/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/components/(.*)",
            "dest": "/components/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}